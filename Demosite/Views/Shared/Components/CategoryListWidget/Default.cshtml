@model List<Demosite.ViewModels.CategoriesListViewModel>
<aside class="single_sidebar_widget post_category_widget">
    <h4 class="widget_title">Category</h4>
    <ul class="list cat-list">
        @foreach (var item in Model)
        {
            <li>
                <a href="script.void()" class="d-flex category-item" data-id="@item.Id">
                    <p>@item.Title</p>
                </a>
            </li>
        }
    </ul>
</aside>
