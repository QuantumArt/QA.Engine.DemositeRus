{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DatabaseQPPostgre": "Server=__postgre.db.server__;Database=__postgre.db.name__;User Id=__postgre.db.user__;Password=__postgre.db.password__"
  },
  "QpSettings": {
    "SiteId": 52,
    "IsStage": "__settings.isStage__",
    "DatabaseType": "__qp.databasetype__",
    "CustomerCode": "__qp.customercode__"
  },
  "OnScreen": {
    "AdminSiteBaseUrl": "__settings.onscreenUrl__"
  },
  "NewsNotificationServiceConfig": {
    "NotificationServiceIsActive": "__notificationService.isActive__", //активировать сервис рассылки
    "BaseURLNewsService": "__notificationService.baseUrlService__", //доменный адресс сервиса новостей
    "Subject": "__notificationService.subject__", //тема письма
    "From": "__notificationService.from__", //от кого
    "SendTimeInterval": "__notificationService.sendTimeInterval__", //интервал с которым срабатывает сервис рассылки
    "SendTimeStart": "__notificationService.sendTimeStart__", //время старта сервиса рассылки
    "CreateRecipientsSize": "__notificationService.createRecipientsSize__", //размер создания получателей в бд
    "MailingPacketSize": "__notificationService.mailingPacketSize__", //размер рассылки писем
    "NumberOfAttemptsSending": "__notificationService.numberOfAttemptsSending__", // попыток отправки писем
    "TimeIntervalOfAttemptsSending": "__notificationService.timeIntervalOfAttemptsSending__", //интервал между попытками отправить заново письма которые не отправились с первого раза
    "EmailConfirmationExpirationTime": "__notificationService.emailConfirmationExpirationTime__", //период подтверждения подписки
    "EmailSender": { //настройка SMTP клиента
      "SmtpServer": "__notificationService.smtpServer__",
      "SmtpPort": "__notificationService.smtpPort__",
      "UserName": "__notificationService.userName__",
      "Password": "__notificationService.password__",
      "Domain": "__notificationService.domain__",
      "UseSsl": "__notificationService.useSsl__"
    }
  },
  "CaptchaSettings": {
    "DefaultKey": "__captchaSettings.defaultKey__",
    "FontWarpFactor": "__captchaSettings.fontWarpFactor__",
    "BackgroundNoiseLevel": "__captchaSettings.backgroundNoiseLevel__",
    "LineNoiseLevel": "__captchaSettings.lineNoiseLevel__",
    "Fonts": [ [ "captchaSettings.fonts" ] ],
    "Colors": [ [ "captchaSettings.colors" ] ],
    "Letters": "__captchaSettings.letters__",
    "Referrer": "__captchaSettings.referrer__",
    "TextLength": "__captchaSettings.textLength__",
    "MaxShift": "__captchaSettings.maxShift__",
    "MinShift": "__captchaSettings.minShift__",
    "MaxAngle": "__captchaSettings.maxAngle__",
    "MinAngle ": "__captchaSettings.minAngle__",
    "FontSizeMax": "__captchaSettings.fontSizeMax__",
    "FontSizeMin": "__captchaSettings.fontSizeMin__",
    "CaptchaWidth": "__captchaSettings.captchaWidth__",
    "CaptchaHeight": "__captchaSettings.captchaHeight__"
  },
  "Cache": {
    "Duration": "__cacheSettings.duration__"
  },
  "FeedbackSettings": {
    "Subject": "__feedbackSettings.subject__", //тема письма
    "DestinationEmail": "__feedbackSettings.destinationEmail__",
    "FromEmail": "__feedbackSettings.fromEmail__"
  }
}
