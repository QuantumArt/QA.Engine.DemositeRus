<!DOCTYPE html>
<html class="no-js">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="description" content="">
  <title>MTS IR</title>
  <link type="image/x-icon" rel="icon" media="" href="~/img/favicon.ico">
  <link type="image/x-icon" rel="shortcut icon" media="" href="~/img/favicon.ico">
  @*<link href="~/css/demosite.min.css" media="screen" rel="stylesheet" type="text/css">*@

  <!-- CSS here -->
  <link rel="stylesheet" href="~/mts.ir.min.css">
  <!-- <link rel="stylesheet" href="css/responsive.css"> -->
</head>
<body>
  <!--[if lte IE 9]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
        <![endif]-->

  @await Component.WidgetZone(Html, "SiteHeaderZone")
  <main class="main">
    @RenderBody()
  </main>



  @await Component.WidgetZone(Html, "SiteFooterZone")

  <!-- JS here -->
  <script src="~/mts.ir.min.js"></script>
  <script src="~/js/banner-slider.js"></script>

  <!-- инициализация аб-тестов -->
  @await Component.InvokeAsync("AbTest")

  <!-- инициализационные скрипты для OnScreen (если используются аб-тесты, должен быть после их инициализации) -->
  @await Component.InvokeAsync("OnScreen")
</body>
</html>
